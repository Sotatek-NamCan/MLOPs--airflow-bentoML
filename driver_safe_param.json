{
  "data_source": "s3://cpnam-s3-tfbackend/data/driver_safety/driver_safety.csv",
  "data_format": "csv",
  "input_schema_version": "v1",
  "model_name": "random_forest_classifier",
  "model_version": "1",
  "hyperparameters": {
    "n_estimators": 200,
    "max_depth": 12,
    "min_samples_split": 4,
    "min_samples_leaf": 2
  },
  "training_scenario": "demo_driver_fleet",
  "target_output_path": "s3://cpnam-s3-tfbackend/models/driver_safety/",
  "test_size": 0.2,
  "random_state": 22,
  "ingestion_config": {
    "object_storage": {
      "enabled": true,
      "bucket": "cpnam-s3-tfbackend",
      "object_key": "data/driver_safety/driver_safety.csv",
      "cache_dir": "data/cache_driver_safety"
    },
    "file_extension": ".csv"
  },
  "target_column": "target_high_risk",
  "data_validation": {
    "min_row_count": 350,
    "required_columns": [
      "driver_id",
      "weekly_miles",
      "avg_speed_mph",
      "incident_count",
      "service_years",
      "safety_score",
      "target_high_risk"
    ],
    "non_null_columns": [
      "driver_id",
      "weekly_miles",
      "avg_speed_mph",
      "incident_count",
      "service_years",
      "safety_score",
      "target_high_risk"
    ],
    "value_ranges": {
      "weekly_miles": {
        "min": 600,
        "max": 3300
      },
      "avg_speed_mph": {
        "min": 30,
        "max": 90
      },
      "incident_count": {
        "min": 0,
        "max": 8
      },
      "safety_score": {
        "min": 40,
        "max": 100
      }
    }
  },
  "hyperparameter_tuning": {
    "enabled": true,
    "n_trials": 30,
    "timeout": 600,
    "search_space": {
      "n_estimators": {
        "type": "int",
        "low": 100,
        "high": 500,
        "step": 20
      },
      "max_depth": {
        "type": "int",
        "low": 5,
        "high": 25
      },
      "min_samples_split": {
        "type": "int",
        "low": 2,
        "high": 8
      },
      "min_samples_leaf": {
        "type": "int",
        "low": 1,
        "high": 5
      }
    }
  }
}
