    "random_state": 42,
    "ingested_dataset_uri": "s3://cpnam-lab/artifacts/iris/ingested/iris.csv",
    "training_base_uri": "s3://cpnam-lab/artifacts/iris",
    "trained_model_uri": "s3://cpnam-lab/artifacts/iris/models/logistic_regression_v1/logistic_regression_v1.pkl",
    "tuning_results_uri": "s3://cpnam-lab/artifacts/iris/hpo/best_params.json"
  }
}