{
  "data_source": "s3://demo-ml-datasets/demos/energy_demand/regional_energy_load.csv",
  "data_format": "csv",
  "input_schema_version": "v1",
  "model_name": "random_forest_regressor",
  "model_version": "2",
  "hyperparameters": {
    "n_estimators": 300,
    "max_depth": 14,
    "min_samples_leaf": 2
  },
  "training_scenario": "demo_energy_forecast",
  "target_output_path": "s3://demo-ml-artifacts/models/energy_demand/",
  "test_size": 0.2,
  "random_state": 7,
  "ingestion_config": {
    "object_storage": {
      "enabled": true,
      "bucket": "demo-ml-datasets",
      "object_key": "demos/energy_demand/regional_energy_load.csv",
      "cache_dir": "demos/cache_energy_demand"
    },
    "file_extension": ".csv"
  },
  "target_column": "total_load_mwh",
  "data_validation": {
    "min_row_count": 400,
    "required_columns": [
      "site_id",
      "avg_temperature_c",
      "relative_humidity_pct",
      "day_of_week",
      "is_holiday",
      "previous_day_load_mwh",
      "renewable_share_pct",
      "total_load_mwh"
    ],
    "non_null_columns": [
      "site_id",
      "avg_temperature_c",
      "relative_humidity_pct",
      "day_of_week",
      "is_holiday",
      "previous_day_load_mwh",
      "renewable_share_pct",
      "total_load_mwh"
    ],
    "value_ranges": {
      "avg_temperature_c": {
        "min": -10,
        "max": 50
      },
      "relative_humidity_pct": {
        "min": 10,
        "max": 100
      },
      "previous_day_load_mwh": {
        "min": 150,
        "max": 700
      },
      "renewable_share_pct": {
        "min": 0,
        "max": 90
      }
    }
  }
}