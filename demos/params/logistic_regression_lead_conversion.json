{
  "data_source": "s3://cpnam-s3-tfbackend/data/lead_conversion_demo.csv",
  "data_format": "csv",
  "input_schema_version": "v1",
  "model_name": "logistic_regression",
  "model_version": "1",
  "hyperparameters": {
    "penalty": "l2",
    "C": 0.7,
    "solver": "lbfgs",
    "max_iter": 300,
    "n_jobs": null
  },
  "training_scenario": "demo_lead_conversion",
  "target_output_path": "s3://cpnam-s3-tfbackend/models/",
  "test_size": 0.2,
  "random_state": 33,
  "ingestion_config": {
    "object_storage": {
      "enabled": true,
      "bucket": "cpnam-s3-tfbackend",
      "object_key": "data/lead_conversion_demo.csv",
      "cache_dir": "data/cache_lead_conversion"
    },
    "file_extension": ".csv"
  },  
  "target_column": "converted",
  "data_validation": {
    "min_row_count": 600,
    "required_columns": [
      "lead_id",
      "sessions_per_week",
      "avg_session_duration_min",
      "email_opens",
      "industry_score",
      "budget_index",
      "converted"
    ],
    "non_null_columns": [
      "sessions_per_week",
      "avg_session_duration_min",
      "email_opens",
      "industry_score",
      "budget_index",
      "converted"
    ],
    "value_ranges": {
      "sessions_per_week": {
        "min": 0,
        "max": 25
      },
      "avg_session_duration_min": {
        "min": 0,
        "max": 25
      },
      "email_opens": {
        "min": 0,
        "max": 60
      },
      "industry_score": {
        "min": 0,
        "max": 1.2
      },
      "budget_index": {
        "min": 0,
        "max": 1.8
      }
    }
  },
  "hyperparameter_tuning": {
    "enabled": true,
    "n_trials": 20,
    "timeout": 600,
    "search_space": {
      "C": {
        "type": "float",
        "low": 0.05,
        "high": 2.5,
        "log": true
      },
      "max_iter": {
        "type": "int",
        "low": 100,
        "high": 400,
        "step": 25
      }
    }
  }
}