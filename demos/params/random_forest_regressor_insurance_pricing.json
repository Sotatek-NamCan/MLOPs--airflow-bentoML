{
  "data_source": "s3://cpnam-s3-tfbackend/data/insurance_pricing_demo.parquet",
  "data_format": "parquet",
  "input_schema_version": "v1",
  "model_name": "random_forest_regressor",
  "model_version": "1",
  "hyperparameters": {
    "n_estimators": 180,
    "max_depth": 14,
    "min_samples_leaf": 2,
    "min_samples_split": 4,
    "n_jobs": -1,
    "random_state": 11
  },
  "training_scenario": "demo_insurance_pricing",
  "target_output_path": "s3://cpnam-s3-tfbackend/models/",
  "test_size": 0.2,
  "random_state": 11,
  "ingestion_config": {
    "object_storage": {
      "enabled": true,
      "bucket": "cpnam-s3-tfbackend",
      "object_key": "data/insurance_pricing_demo.parquet",
      "cache_dir": "data/cache_insurance_pricing"
    },
    "file_extension": ".parquet"
  },
  "target_column": "premium_usd",
  "data_validation": {
    "min_row_count": 500,
    "required_columns": [
      "policy_id",
      "driver_age",
      "vehicle_age",
      "annual_mileage",
      "accidents_last_3y",
      "credit_score",
      "premium_usd"
    ],
    "non_null_columns": [
      "driver_age",
      "vehicle_age",
      "annual_mileage",
      "accidents_last_3y",
      "credit_score",
      "premium_usd"
    ],
    "value_ranges": {
      "driver_age": {
        "min": 18,
        "max": 80
      },
      "vehicle_age": {
        "min": 0,
        "max": 20
      },
      "annual_mileage": {
        "min": 2000,
        "max": 40000
      },
      "premium_usd": {
        "min": 100,
        "max": 2000
      }
    }
  },
  "hyperparameter_tuning": {
    "enabled": true,
    "n_trials": 25,
    "timeout": 900,
    "search_space": {
      "n_estimators": {
        "type": "int",
        "low": 80,
        "high": 400,
        "step": 20
      },
      "max_depth": {
        "type": "int",
        "low": 6,
        "high": 24
      },
      "min_samples_leaf": {
        "type": "int",
        "low": 1,
        "high": 6
      },
      "min_samples_split": {
        "type": "int",
        "low": 2,
        "high": 10
      }
    }
  }
}