{
  "data_source": "demos/datasets/fruit_quality_demo.csv",
  "data_format": "csv",
  "input_schema_version": "v1",
  "model_name": "random_forest_classifier",
  "model_version": "1",
  "hyperparameters": {
    "n_estimators": 300,
    "max_depth": 10,
    "min_samples_split": 4,
    "min_samples_leaf": 2,
    "random_state": 22
  },
  "training_scenario": "demo_fruit_quality",
  "target_output_path": "s3://demo-artifacts/models/fruit_quality/",
  "test_size": 0.25,
  "random_state": 22,
  "ingestion_config": {
    "object_storage": {
      "enabled": false
    },
    "file_path": "demos/datasets/fruit_quality_demo.csv",
    "file_extension": ".csv"
  },
  "target_column": "fruit_type",
  "data_validation": {
    "min_row_count": 240,
    "required_columns": [
      "weight_g",
      "length_cm",
      "color_score",
      "sweetness",
      "firmness",
      "fruit_type"
    ],
    "non_null_columns": [
      "weight_g",
      "length_cm",
      "color_score",
      "sweetness",
      "firmness",
      "fruit_type"
    ],
    "value_ranges": {
      "weight_g": {
        "min": 90,
        "max": 330
      },
      "length_cm": {
        "min": 5.5,
        "max": 22.0
      },
      "color_score": {
        "min": 0.4,
        "max": 1.0
      },
      "sweetness": {
        "min": 5.0,
        "max": 10.0
      },
      "firmness": {
        "min": 2.5,
        "max": 9.5
      }
    },
    "allowed_values": {
      "fruit_type": [
        "apple",
        "banana",
        "orange",
        "mango"
      ]
    }
  }
}
