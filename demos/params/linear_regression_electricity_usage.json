{
  "data_source": "demos/datasets/monthly_electricity_usage_demo.parquet",
  "data_format": "parquet",
  "input_schema_version": "v1",
  "model_name": "linear_regression",
  "model_version": "1",
  "hyperparameters": {
    "fit_intercept": true,
    "copy_X": true
  },
  "training_scenario": "demo_electricity_forecast",
  "target_output_path": "s3://demo-artifacts/models/monthly_electricity/",
  "test_size": 0.2,
  "random_state": 11,
  "ingestion_config": {
    "object_storage": {
      "enabled": false
    },
    "file_path": "demos/datasets/monthly_electricity_usage_demo.parquet",
    "file_extension": ".parquet"
  },
  "target_column": "electricity_kwh_month",
  "data_validation": {
    "min_row_count": 200,
    "required_columns": [
      "household_size",
      "floor_area_m2",
      "ac_hours_per_day",
      "num_devices",
      "avg_temp_c",
      "electricity_kwh_month"
    ],
    "non_null_columns": [
      "household_size",
      "floor_area_m2",
      "ac_hours_per_day",
      "num_devices",
      "avg_temp_c",
      "electricity_kwh_month"
    ],
    "value_ranges": {
      "household_size": {
        "min": 1,
        "max": 6
      },
      "floor_area_m2": {
        "min": 45,
        "max": 260
      },
      "ac_hours_per_day": {
        "min": 0,
        "max": 18
      },
      "num_devices": {
        "min": 5,
        "max": 30
      },
      "avg_temp_c": {
        "min": 16,
        "max": 38
      },
      "electricity_kwh_month": {
        "min": 180,
        "max": 1200
      }
    }
  }
}
