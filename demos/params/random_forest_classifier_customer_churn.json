{
  "data_source": "s3://cpnam-s3-tfbackend/data/customer_churn_demo.csv",
  "data_format": "csv",
  "input_schema_version": "v1",
  "model_name": "random_forest_classifier",
  "model_version": "1",
  "hyperparameters": {
    "n_estimators": 180,
    "max_depth": 12,
    "min_samples_split": 3,
    "min_samples_leaf": 1,
    "n_jobs": -1,
    "random_state": 22
  },
  "training_scenario": "demo_customer_churn",
  "target_output_path": "s3://cpnam-s3-tfbackend/models/",
  "test_size": 0.25,
  "random_state": 22,
  "ingestion_config": {
    "object_storage": {
      "enabled": true,
      "bucket": "cpnam-s3-tfbackend",
      "object_key": "data/customer_churn_demo.csv",
      "cache_dir": "data/cache_customer_churn"
    },
    "file_extension": ".csv"
  },
  "target_column": "churned",
  "data_validation": {
    "min_row_count": 600,
    "required_columns": [
      "customer_id",
      "tenure_months",
      "avg_monthly_usage_hours",
      "num_support_tickets",
      "monthly_charge",
      "contract_type",
      "churned"
    ],
    "non_null_columns": [
      "tenure_months",
      "avg_monthly_usage_hours",
      "num_support_tickets",
      "monthly_charge",
      "churned"
    ],
    "value_ranges": {
      "tenure_months": {
        "min": 0,
        "max": 130
      },
      "avg_monthly_usage_hours": {
        "min": 5,
        "max": 90
      },
      "num_support_tickets": {
        "min": 0,
        "max": 20
      },
      "monthly_charge": {
        "min": 10,
        "max": 200
      }
    }
  },
  "hyperparameter_tuning": {
    "enabled": true,
    "n_trials": 30,
    "timeout": 900,
    "search_space": {
      "n_estimators": {
        "type": "int",
        "low": 100,
        "high": 400,
        "step": 20
      },
      "max_depth": {
        "type": "int",
        "low": 6,
        "high": 24
      },
      "min_samples_split": {
        "type": "int",
        "low": 2,
        "high": 8
      },
      "min_samples_leaf": {
        "type": "int",
        "low": 1,
        "high": 5
      }
    }
  }
}