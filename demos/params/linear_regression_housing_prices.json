{
  "data_source": "s3://cpnam-s3-tfbackend/data/housing_prices_demo.csv",
  "data_format": "csv",
  "input_schema_version": "v1",
  "model_name": "linear_regression",
  "model_version": "1",
  "hyperparameters": {
    "fit_intercept": true,
    "copy_X": true,
    "n_jobs": null
  },
  "training_scenario": "demo_housing_prices",
  "target_output_path": "s3://cpnam-s3-tfbackend/models",
  "test_size": 0.25,
  "random_state": 7,
  "ingestion_config": {
    "object_storage": {
      "enabled": true,
      "bucket": "cpnam-s3-tfbackend",
      "object_key": "data/housing_prices_demo.csv",
      "cache_dir": "data/cache_housing_prices"
    },
    "file_extension": ".csv"
  },
  "target_column": "price",
  "data_validation": {
    "min_row_count": 500,
    "required_columns": [
      "listing_id",
      "square_feet",
      "bedrooms",
      "bathrooms",
      "year_built",
      "walk_score",
      "lot_size_acres",
      "price"
    ],
    "non_null_columns": [
      "square_feet",
      "bedrooms",
      "bathrooms",
      "year_built",
      "price"
    ],
    "value_ranges": {
      "square_feet": {
        "min": 500,
        "max": 4500
      },
      "bedrooms": {
        "min": 1,
        "max": 7
      },
      "bathrooms": {
        "min": 1,
        "max": 5
      },
      "year_built": {
        "min": 1950,
        "max": 2025
      },
      "price": {
        "min": 50000,
        "max": 1500000
      }
    }
  },
  "hyperparameter_tuning": {
    "enabled": false,
    "n_trials": 0,
    "timeout": 0,
    "search_space": {}
  }
}