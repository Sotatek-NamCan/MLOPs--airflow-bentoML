{
  "data_source": "s3://cpnam-s3-tfbackend/demo_cases/marketing_leads/marketing_leads.csv",
  "data_format": "csv",
  "input_schema_version": "v1",
  "model_name": "logistic_regression",
  "model_version": "1",
  "hyperparameters": {
    "C": 1.0,
    "max_iter": 200
  },
  "training_scenario": "leads_demo",
  "target_output_path": "s3://cpnam-s3-tfbackend/models/marketing_leads/",
  "test_size": 0.2,
  "random_state": 7,
  "ingestion_config": {
    "object_storage": {
      "enabled": true,
      "bucket": "cpnam-s3-tfbackend",
      "object_key": "demo_cases/marketing_leads/marketing_leads.csv",
      "cache_dir": "data/demo_cases/cache_marketing"
    },
    "file_extension": ".csv"
  },
  "target_column": "conversion",
  "data_validation": {
    "min_row_count": 3,
    "required_columns": [
      "lead_id",
      "region",
      "annual_spend",
      "interactions",
      "conversion"
    ],
    "non_null_columns": [
      "lead_id",
      "region",
      "annual_spend",
      "interactions",
      "conversion"
    ]
  },
  "hyperparameter_tuning": {
    "enabled": true,
    "n_trials": 25,
    "timeout": 300,
    "search_space": {
      "C": {
        "type": "float",
        "low": 0.01,
        "high": 10,
        "log": true
      },
      "max_iter": {
        "type": "int",
        "low": 100,
        "high": 800,
        "step": 50
      }
    }
  }
}
