{
  "data_source": "s3://cpnam-s3-tfbackend/demo_cases/driver_safety/driver_safety.csv",
  "data_format": "csv",
  "input_schema_version": "v1",
  "model_name": "random_forest_classifier",
  "model_version": "1",
  "hyperparameters": {
    "n_estimators": 150,
    "max_depth": 10,
    "min_samples_split": 3,
    "min_samples_leaf": 1
  },
  "training_scenario": "driver_demo",
  "target_output_path": "s3://cpnam-s3-tfbackend/models/driver_demo/",
  "test_size": 0.2,
  "random_state": 11,
  "ingestion_config": {
    "object_storage": {
      "enabled": true,
      "bucket": "cpnam-s3-tfbackend",
      "object_key": "demo_cases/driver_safety/driver_safety.csv",
      "cache_dir": "data/demo_cases/cache_driver"
    },
    "file_extension": ".csv"
  },
  "target_column": "target_high_risk",
  "data_validation": {
    "min_row_count": 3,
    "required_columns": [
      "driver_id",
      "weekly_miles",
      "avg_speed_mph",
      "incident_count",
      "service_years",
      "safety_score",
      "target_high_risk"
    ],
    "non_null_columns": [
      "driver_id",
      "weekly_miles",
      "avg_speed_mph",
      "incident_count",
      "service_years",
      "safety_score",
      "target_high_risk"
    ]
  },
  "hyperparameter_tuning": {
    "enabled": true,
    "n_trials": 20,
    "timeout": 300,
    "search_space": {
      "n_estimators": {
        "type": "int",
        "low": 80,
        "high": 400,
        "step": 20
      },
      "max_depth": {
        "type": "int",
        "low": 4,
        "high": 20
      },
      "min_samples_split": {
        "type": "int",
        "low": 2,
        "high": 8
      },
      "min_samples_leaf": {
        "type": "int",
        "low": 1,
        "high": 4
      }
    }
  }
}
