{
  "data_source": "s3://cpnam-s3-tfbackend/demo_cases/iot_sensors/iot_readings.csv",
  "data_format": "csv",
  "input_schema_version": "v1",
  "model_name": "random_forest_classifier",
  "model_version": "1",
  "hyperparameters": {
    "n_estimators": 120,
    "max_depth": 8,
    "min_samples_split": 2
  },
  "training_scenario": "iot_fault_detection",
  "target_output_path": "s3://cpnam-s3-tfbackend/models/iot_sensors/",
  "test_size": 0.25,
  "random_state": 5,
  "ingestion_config": {
    "object_storage": {
      "enabled": true,
      "bucket": "cpnam-s3-tfbackend",
      "object_key": "demo_cases/iot_sensors/iot_readings.csv",
      "cache_dir": "data/demo_cases/cache_iot"
    },
    "file_extension": ".csv"
  },
  "target_column": "fault",
  "data_validation": {
    "min_row_count": 3,
    "required_columns": [
      "device_id",
      "temperature_c",
      "vibration_mm_s",
      "pressure_kpa",
      "fault"
    ],
    "non_null_columns": [
      "device_id",
      "temperature_c",
      "vibration_mm_s",
      "pressure_kpa",
      "fault"
    ]
  },
  "hyperparameter_tuning": {
    "enabled": true,
    "n_trials": 20,
    "timeout": 300,
    "search_space": {
      "n_estimators": {
        "type": "int",
        "low": 80,
        "high": 400,
        "step": 20
      },
      "max_depth": {
        "type": "int",
        "low": 3,
        "high": 18
      },
      "min_samples_split": {
        "type": "int",
        "low": 2,
        "high": 8
      }
    }
  }
}
